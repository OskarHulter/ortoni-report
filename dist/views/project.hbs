                </summary>
                <ul class="mt-4 mb-4">
                    {{#each this}}
                    <li class="media {{#if (eq status 'skipped')}}is-hidden{{/if}}" data-suite-name="{{suite}}"
                        data-test-duration="{{duration}}" data-test-tags="{{joinWithSpace testTags}}"
